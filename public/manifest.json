{
    "name": "Matrix Highlight",
    "description": "Matrix-based page highlighting",
    "version": "0.1.0",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [ "storage", "contextMenus", "activeTab", "tabs", "http://*/*", "https://*/*"],
    "content_scripts": [
        {
            "js": ["autorun.js"],
            "matches": [
                "http://*/*",
                "https://*/*"
            ]
        }
   ]
}
